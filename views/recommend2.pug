                font-size: 11px;
                font-weight: 400;
            }
            .line1{
                padding: 11px;
            }
            .line2{
                padding-top: 7px;
            }
            .chec-radio label.radio-inline input[type="radio"] {
                display: none;
            }
            .chec-radio label.radio-inline input[type="radio"]:checked+div {
                color: #8fd2c9;
                background-color: white;
                border-radius: 3px;
                border-color: transparent;
                font-weight: bold;
            }
            .chec-radio label.radio-inline input[type="radio"][disabled] :checked+div 
            {
                border-color: purple;
            }
            #type{
