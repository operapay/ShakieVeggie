            .chec-radio .radio-inline .clab {
                cursor: pointer;
                background: transparent;
                padding-top: 16px;
                text-align: center;
                text-transform: uppercase;
                color: white;
                position: relative;
                width: 87.75px;
                height: 48px;
                line-height:93%;
                float: left;
                margin: 0;
                /*margin-bottom: 5px;*/
                border-radius: 2px;
                /*border-style: solid;*/
